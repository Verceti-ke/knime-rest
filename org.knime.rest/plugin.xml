<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.knime.rest.authentication" name="REST Authentication" schema="schema/org.knime.rest.authentication.exsd"/>
    <extension point="org.knime.workbench.repository.categories">
        <category icon="icons/rest_category.png" level-id="rest" name="REST Web Services" path="/labs/"/>
    </extension>
    <extension point="org.knime.workbench.repository.nodes">
        <node category-path="/labs/rest" factory-class="org.knime.rest.nodes.get.RestGetNodeFactory" id="org.knime.rest.nodes.get.RestGetNodeFactory"/>
    </extension>
    <extension
          id="org.knime.rest.authentication.basic"
          point="org.knime.rest.authentication">
       <AuthenticateEachRequest
             class="org.knime.rest.internals.BasicAuthentication"
             deprecated="false"
             supportsDistributedExecution="true">
       </AuthenticateEachRequest>
       <AuthenticateEachRequest
             class="org.knime.rest.internals.DigestAuthentication"
             deprecated="false"
             supportsDistributedExecution="true">
       </AuthenticateEachRequest>
    </extension>
</plugin>
